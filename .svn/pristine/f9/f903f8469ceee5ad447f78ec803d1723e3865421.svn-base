}

.login-body .wrapbj .loginbox .account-input {
	margin-top: 20px;
}

.login-body .wrapbj .loginbox .password-input {
	margin-top: 15px;
}

.login-body .wrapbj .loginbox .account-img {
    width: 28px;
    height: 20px;
    margin-top:-72px;
    margin-left:18px;
}

.login-body .wrapbj .loginbox .password-img {
    width: 25px;
    height: 19px;
    margin-top:-72px;
    margin-left:22px;
}

.login-body .wrapbj .loginbox .account-input::-webkit-input-placeholder {
    font-size: 16px;
    color: #7f7d87;
}

.login-body .wrapbj .loginbox .account-input::-moz-input-placeholder {
    font-size: 16px;
    color: #7f7d87;
}

.login-body .wrapbj .loginbox .password-input::-webkit-input-placeholder {
    font-size: 16px;
    color: #7f7d87;
}

.login-body .wrapbj .loginbox .password-input::-moz-input-placeholder {
    font-size: 16px;
    color: #7f7d87;
}

.login-body .wrapbj .loginbox .btn-default {
	margin-top: 40px;
	margin-bottom: 10px;
	width: 100%;
	height: 48px;
	background-color: #929099;
	border:none;
	outline: none;
    font-size: 18px;
    color: #fff;
}
.login-body .wrapbj .loginbox .btn-default:hover{
    background-color: #b6b4be;
}
.login-body .wrapbj .bank-authentication{
  width: 64px;
  height: 64px;
  margin: 0 auto;
  display: block;
}
.login-body .wrap .login-text {
	color: #a10904;
	font-family: SimHei;
	font-size: 18px;
}

.login-body .wrap .rf-text {
	color: #f8b4b5;
}

/* 小圆点样式 */

#dropIcon {
  z-index: 9999;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  top: 600px;
  left: 83%;
  position: absolute;
}
#dropImg{
  width: 50px;
  height: 50px;
  background: url();
}
.first-menu {
  z-index: 998;
  width: 240px;
  background-color: #000;
  border-radius: 10px;
  position: relative;
  color: #a4a4a5;
  /* margin-top: -80px;
  margin-left: -80px; */
  display: none;
  overflow-y: auto;
}
.first-menu img {
  width: 24px;
  height: 24px;
  display: block;
}
.first-menu .icon-title {
  width: 100%;
  height: 15%;
  font-size: 18px;
  padding-left: 40px;
  line-height: 49px;
}
.icon-title .title-stars{
  margin-top:10px;
  margin-right: 30px;
  float: right;
}
.first-menu .icon-default {
  width:100%;
  height:20%;
}
.first-menu .icon-wrap {
  margin-top: 3px;
  width:100%;
  height:60%;
}
.first-menu ul {
  list-style: none;
  padding-left: 3px;
  margin-top: 15px;
}
.first-menu ul li {
  float: left;
  width: 33%;
  margin-bottom: 5px;
}
.first-menu ul li a {
  margin-bottom: 8px;
  line-height: 20px;
  color: #a4a4a5;
  display: block;
}
.first-menu hr {
  margin-top: -5px;
  margin-left: 30px;
  margin-right: 30px;
  height: 0;
  border-top:1px solid #383a3d;
  border-right: 0;
  border-bottom: 0;
  border-left: 0;
}
#firstMenu::-webkit-scrollbar-track     {/* 划动块top的颜色 */
  background:#8DB6CD;
}
#firstMenu::-webkit-scrollbar-track-piece {  /* 划动块背景的颜色 */
  background:#DCDCDC;
  border-radius: 10px;
}
#firstMenu::-webkit-scrollbar-thumb{/* 划动块bottom  的颜色 */
  background:#8DB6CD;
  border-radius:4px;
}
#firstMenu::-webkit-scrollbar-corner {
  background:#0000EE;
}
#firstMenu::-webkit-scrollbar-resizer  {
  background:#0000EE;
}
#firstMenu::-webkit-scrollbar { width: 12px;}/* 滚动槽 */
#firstMenu::-webkit-scrollbar-track { border-radius: 10px;}/* 滚动条滑块 */
#firstMenu::-webkit-scrollbar-thumb {    border-radius: 10px;}
#firstMenu::-webkit-scrollbar-thumb:window-inactive {   }

