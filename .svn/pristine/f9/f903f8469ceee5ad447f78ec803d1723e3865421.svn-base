.login-body .wrap {
    margin-top: 100px;
}

.login-body .wrap .logo {
	width: 100%;
	height: 100%;
}

.login-body .wrapbj .loginbox input {
	width: 100%;
	height: 48px;
    outline: none;
    background-size: 55px 50px;
    background-repeat: no-repeat;
    background-position: 25px 7px;
    padding-left: 55px;
    font-size: 16px;
