/*  过渡页样式  */
.login-body .transitionpage{
  height: 759px;
  margin-top: 0;
  background: url(../images/transitionpage.jpg) center no-repeat;
}
@media (min-width: 1600px){
  .login-body .transitionpage{
    height: 759px;
    margin-top: 0;
  }
}
@media (min-width: 1605px) and (max-width: 1680px) {
  .login-body .transitionpage{
    height: 759px;
    margin-top: 80px;
  }
}
@media (min-width: 1680px) and (max-width: 1920px) {
  .login-body .transitionpage{
    height: 759px;
    margin-top: 120px;
  }
}
@media (min-width: 1920px) and (max-width: 2560px) {
  .login-body .transitionpage{
    height: 759px;
    margin-top: 170px;
  }
}

/*  鉴权样式  */
.login-body .authority{
  height: 759px;
  margin-top: 0;
  background: url(../images/bj.jpg) center no-repeat;
}
@media (min-width: 1600px){
  .login-body .authority{
    height: 759px;
    margin-top: 0;
  }
  #login-body-id{
    margin-top: 0;
  }
}
@media (min-width: 1605px) and (max-width: 1680px) {
  .login-body .authority{
    height: 759px;
    margin-top: 80px;
  }
  #login-body-id{
    margin-top: 80px;
  }
}
@media (min-width: 1680px) and (max-width: 1920px) {
  .login-body .authority{
    height: 759px;
    margin-top: 120px;
  }
  #login-body-id{
    margin-top: 120px;
  }
}
@media (min-width: 1920px) and (max-width: 2560px) {
  .login-body .authority{
    height: 759px;
    margin-top: 170px;
  }
  #login-body-id{
    margin-top: 170px;
  }
}
.login-body .authority .authoribox{
