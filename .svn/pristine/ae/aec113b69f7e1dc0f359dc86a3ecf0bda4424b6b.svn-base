  function poollist(data){
    //$(this).addClass('on').siblings().removeClass('on');
    $("#pool"+data).addClass('on').siblings().removeClass('on');
    $("#listshow"+data).show().siblings().hide();
  }
  $(".closecollection").click(function(event) {
  	$(this).siblings().show();
  	$(this).hide();
  });
