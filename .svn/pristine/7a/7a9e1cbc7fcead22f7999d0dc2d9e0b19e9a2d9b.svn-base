/*弹框背景*/
.kaijitupian-one-class .starsall .sevenStarsBg {
//padding: 10px;
  background-color: rgba(120, 120, 120, 0.5);
  position: absolute;
  padding-top: 6px;
  padding-bottom: 6px;
  max-width: 300px;
  min-width: 100px;
  color: #000;
  font-size: 13px;
  top: 90px;
  right: -0px;
  z-index: 99;
  border-radius: 8px;
}
.kaijitupian-one-class .starsall .sevenStarsBg ol li{
  padding-left: 15px;
}
.kaijitupian-one-class .starsall .sevenStarsBg .bgTitle {
  border-bottom: 1px solid silver;
  padding: 10px;
  padding-left: 0;
  font-size: 18px;
}

.kaijitupian-one-class .starsall .sevenStarsBg ol li span{
  line-height: 26px;
  color: #bfbfbf;
  cursor: pointer;
}
.kaijitupian-one-class .starsall .sevenStarsBg ol li a:hover {
  background-color: rgba(0,0,0,0.3);
  color:white;
}

