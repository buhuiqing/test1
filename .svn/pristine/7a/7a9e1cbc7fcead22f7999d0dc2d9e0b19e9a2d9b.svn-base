/* 圆形进度 */
.layout {
  width: 250px;
  height: 250px; 
  margin:0 auto;
  background: #2f3843;
  display: block;
}
#box {
  position: absolute;
  color: #fff;
}
.layout .layoutbox{
  position: relative;
}
.indicatorContainer{
	width:360px;
	margin:0 auto;
	position: absolute;
	z-index: 2;
}
.indicatorContainer canvas{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
#indicatorContainer1 {
	width:250px;
	margin:0 auto;
	position: absolute;
	z-index: 2;
}
#indicatorContainer1 canvas{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
