.personal-center-help-body {
	background-color: #c6caca;
}
.personal-center-help-body {
	background-color: #fff;
	min-width: 1100px;
	font-size: 14px;
}
.personal-center-help-body .personal-center-help-logo{
	margin: 80px 0 30px;
    height: 28px;
}
.personal-center-help-body .title {
    height: 100px;
    margin-bottom: 25px;
    background-color: #004b80;
}
.personal-center-help-body .title h4{
	height: 100px;
	padding-left: 60px;
	line-height: 100px;
	color: #fff;
}
.personal-center-help-body .helpbox{
	width: 1000px;
	margin:0 auto;
}
.personal-center-help-body .helpbox li{
	padding-top: 15px;
	padding-bottom: 15px;
	border-bottom: 1px solid #e2e2e2;
}
.personal-center-help-body .helpbox .helpbox-t{
	line-height: 20px;
	margin-bottom: 15px;
}
.personal-center-help-body .helpbox .helpbox-t span{
   padding-right: 10px;
   font-weight: bold;
   color: #06396d;
   display: table-cell;
   float: left;
}
.personal-center-help-body .helpbox .helpbox-t p{
	height: 20px;
    line-height: 20px;
    font-size: 14px;
    font-weight: bold;
    font-family: Microsoft YaHei;
    letter-spacing: 0.2px;
    color: #000000;
    display: table-cell;
}
.personal-center-help-body .helpbox .helpconter span{
   padding-right: 10px;
   font-weight: bold;
   color: #730000;
   display: table-cell;
}
.personal-center-help-body .helpbox .helpconter p{
  line-height: 24px;
  letter-spacing: 0.2px;
  color: #4a4a4a;
  display: table-cell;
}


